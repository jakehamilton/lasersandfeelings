    "@leverage/plugin-http": "^4.0.3",
    "@leverage/plugin-websocket": "^2.0.4",
    "@littlethings/log": "^2.5.0",
    "@preact/preset-vite": "^2.1.0",
    "compression": "^1.7.4",
    "goober": "^2.0.38",
